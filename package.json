{
  "name": "digisigner-test-task",
  "version": "1.0.0",
  "scripts": {
    "start": "ng serve --hmr",
    "build:dev": "ng build --configuration development && node scripts/compress.js",
    "build:prod": "ng build && node scripts/compress.js",
    "update-lock": "npm i --package-lock-only",
    "prettier:html": "npx prettier -w \"./src/**/**.html\" --cache",
    "prettier:styles": "npx prettier -w \"./src/**/**.scss\" --cache",
    "prettier:ts": "npx prettier -w \"./src/**/**.ts\" --cache",
    "eslint": "eslint . --ext .html,.ts",
    "stylelint": "npx stylelint \"./src/**/**.scss\" --cache --fix",
    "test": "jest",
    "generate-i18n": "npx ng extract-i18n --out-file src/locale/messages.ru.xlf",
    "generate-icons": "svg-to-ts-object --config ./src/app/modules/svg-icon/icons.config.json",
    "prepare": "husky install"
  },
  "private": true,
