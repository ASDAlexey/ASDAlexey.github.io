{
  "name": "currency-calculator",
  "version": "0.0.0",
  "author": "Alexey Popov <asdalexey@yandex.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ASDAlexey/ASDAlexey.github.io"
  },
  "scripts": {
    "ng-serve": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng serve",
    "ng-build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build",
    "server": "npm run ng-serve -- --configuration development -- --hmr",
    "build:dev": "ng build --configuration development && npm run html-minify && npm run compress",
    "build:prod": "ng build --configuration production && npm run html-minify && npm run compress",
    "html-minify": "node ./scripts/html-minify.js",
    "compress": "node scripts/compress.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@ngrx/component": "^12.5.1",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/entity": "^12.5.1",
    "@ngrx/router-store": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "husky": "^7.0.4",
    "nanoid": "^3.1.31",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@types/jasmine": "~3.10.1",
    "@types/node": "^16.11.6",
    "cheerio": "^1.0.0-rc.10",
    "html-minifier": "^4.0.0",
    "jasmine-core": "~3.10.1",
    "karma": "~6.3.6",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "typescript": "~4.4.4"
  },
  "packageManager": "yarn@3.1.0"
}
