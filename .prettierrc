{
  "plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-css-order", "@trivago/prettier-plugin-sort-imports"],
  "bracketSpacing": true,
  "printWidth": 140,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "cssDeclarationSorterOrder": "smacss",
  "attributeGroups": ["^#", "$ANGULAR_STRUCTURAL_DIRECTIVE", "$DEFAULT", "$ANGULAR_INPUT", "$ANGULAR_TWO_WAY_BINDING", "$ANGULAR_OUTPUT"],
  "attributeSort": "ASC",
  "importOrder": ["^@app/(.*)$", "^@env/(.*)$", "^[./]", "^[../]"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
